{
  "name": "personal-icons",
  "version": "1.0.9",
  "description": "A personal icon library.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "generate:icons": "ts-node scripts/generate-icons.ts",
    "build:lib": "tsup src/lib/index.ts --format cjs,esm --dts --clean",
    "build:site": "vite build",
    "build": "npm run generate:icons && npm run build:lib && npm run build:site"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.5.3",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^3.9.1",
    "tsup": "^8.2.4"
  }
}
